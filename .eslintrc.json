{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base",
        "eslint:recommended"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "no-underscore-dangle": ["error", {
            "allow": ["_id"]
            }],
        "semi": ["error", "always"],
        "quotes": ["error", "double"]
    }
}
